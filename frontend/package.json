    "@tailwindcss/vite": "^4.1.18",
    "@tanstack/table-core": "^8.21.3",
    "bits-ui": "^2.15.5",
    "clsx": "^2.1.1",
    "embla-carousel-svelte": "^8.6.0",
    "eslint": "^9.39.2",
