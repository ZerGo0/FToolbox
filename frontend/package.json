    "@tailwindcss/vite": "^4.1.13",
    "@tanstack/table-core": "^8.21.3",
    "bits-ui": "^2.11.4",
    "clsx": "^2.1.1",
    "embla-carousel-svelte": "^8.6.0",
    "eslint": "^9.36.0",
